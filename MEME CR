import random

meme_dict = {
            "КРИНЖ": " Это слово озночает:Что-то очень странное или стыдное",
            "ЛОЛ": "Это слово озночает:Что-то очень смешное",
            "РОФЛ": "Это слово озночает:это по простому шутка",
            "ЩИЩ":"Это слово озночает:одобрения или восторг",
            }
word = input("Введите непонятное слово (большими буквами!): ")
if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("простите мы вас не поняли")
